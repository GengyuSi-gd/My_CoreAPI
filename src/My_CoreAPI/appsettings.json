{
  "ApplicationName": "My_CoreAPI",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=My_CoreAPI_DB;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "RabbitMQSettings": {
    "HostName": "localhost",
    "Port": 5672,
    "RabbitMQCertSubject": "svc_bos_core",
    "UseSSL": false,
    "UserName": "guest",
    "Password": "guest",
    "VirtualHost": "/",
    "ExchangeName": "my_exchange",
    "QueueName": "my_request-handler_queue",
    "RabbitMQResponseQueueName": "",
    "RoutingKey": "*",
    "ExchangeType": "topic",
    "DurableRequestHandlerExchangeName": false,
    "AutoDeleteRequestHandlerExchangeName": false,
    "RequestHandlerAllQueueName": "request-handler-all",
    "DurableRequestHandlerAllQueueName": false,
    "AutoDeleteRequestHandlerAllQueueName": false,
    "ExclusiveRequestHandlerAllQueueName": false
  }
}
